<% let body = `
    <div class="container mt-3">
        <div class="row">
            <% for (let i = 0; i <= 5; i++) { %>
                <div class="col-lg-4">
                    <div class="card">
                        <img src="/images/slide-1.jpg" class="card-img-top" alt="My Image">
                        <div class="card-body">
                            <h5 class="card-title">Card title</h5>
                            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                            <a href="#" class="btn btn-primary">Go somewhere</a>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
` %>

<%- include( './layout.ejs' , { styles: null, scripts: null, body: body } ) %>